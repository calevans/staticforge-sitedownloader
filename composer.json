{
    "name": "calevans/staticforge-sitedownloader",
    "description": "StaticForge Feature: SiteDownloader",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Calevans\\StaticForgeSiteDownloader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calevans\\StaticForgeSiteDownloader\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4",
        "league/html-to-markdown": "^5.1",
        "symfony/dom-crawler": "^8.0",
        "eicc/utils": "^1.2",
        "symfony/console": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "staticforge": {
            "feature": "Calevans\\StaticForgeSiteDownloader\\Feature"
        }
    }
}